{
    "name": "thedataist/drill-connector",
    "description": "Objects that allow you to programmatically connect to Apache Drill.",
    "keywords": [
        "Drill",
        "database",
        "data exploration",
        "data analysis",
        "views",
        "Apache Drill",
        "Big Data",
        "Data Science",
        "SQL"
    ],
    "homepage": "http://thedataist.com",
    "license": "Apache-2.0",
    "authors" : [
        {
            "name": "Charles Givre",
            "email": "cgivre@thedataist.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "repositories" : [{
      "type": "vcs",
      "url": "https://github.com/cgivre/drill-php"
      }
    ],
    "autoload": {
      "psr-4": { "thedataist\\Drill\\": "src/" },
      "classmap": ["src/"]
    },
    "require": {
        "php": "^7.4||^8.0",
        "ext-curl": "*",
        "ext-json": "*"
    }
}
